{
  "name": "react-project-starter",
  "description": "React Project Starter",
  "keywords": [
    "ReactJs boilerplate",
    "Frontend Architecture"
  ],
  "version": "0.1.0",
  "private": false,
  "homepage": "https://admin.wishforhealth.id/",
  "engines": {
    "node": "14 || 15 || 16",
    "npm": ">=6"
  },
  "author": {
    "name": "Wibisana Putra Setyanegara",
    "email": "eizisana@gmail.com",
    "url": "https://www.linkedin.com/in/isanawib/"
  },
  "bugs": {
    "email": "eizisana@gmail.com",
    "url": "https://github.com/isanaID/react-project-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isanaID/react-project-starter.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build --profile",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx prettier --write -c ./src && npx eslint './src' --quiet --fix",
    "build:dev": "REACT_APP_STAGE=development react-scripts build --profile",
    "security:audit": "npm audit",
    "packages:upgrade": "npm upgrade --latest",
    "setup": "node setup"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./scripts/pre-commit",
      "pre-push": "node ./scripts/pre-push"
    }
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.4",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "axios": "^0.26.1",
    "buffer": "^6.0.3",
    "formik": "^2.2.9",
    "framer-motion": "^6.2.9",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.19",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "react-table": "^7.8.0",
    "recharts": "^2.1.16",
    "sweetalert2": "^11.4.24",
    "typescript": "^4.6.3",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11",
    "zustand": "^3.7.2"
  },
  "devDependencies": {
    "@types/react-table": "^7.7.12",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "4.3.8",
    "prettier": "2.6.2"
  }
}
